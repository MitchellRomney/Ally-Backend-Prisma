datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  userId Int @id @default(autoincrement())
  username String? @unique
  password String
  email String @unique
  summoners Summoner[]
  isAdmin Boolean
}

model Summoner {
  id Int @id @default(autoincrement())
  allyId String @unique
  allyName String @unique
  summonerName String
  summonerId String
  puuid String
  accountId String
  profileIconId Int
  summonerLevel Int
  server String
}