  item6                           Int      @default(0)
  killingSprees                   Int      @default(0)
  kills                           Int      @default(0)
  largestCriticalStrike           Int      @default(0)
  largestKillingSpree             Int      @default(0)
  largestMultiKill                Int      @default(0)
  longestTimeSpentLiving          Int      @default(0)
  magicDamageDealt                Int      @default(0)
